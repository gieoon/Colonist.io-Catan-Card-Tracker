{
    "name": "Colonist.io Catan Card Tracker",
    "description": "Track cards in your colonist.io Catan games.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["tabs","activeTab","scripting"],
    "content_scripts": [ {
        "matches": ["https://colonist.io/*"],
        "css": ["colonist_styles.css"],
        "js": ["js/colonist_script.js"]
    } ]
}
